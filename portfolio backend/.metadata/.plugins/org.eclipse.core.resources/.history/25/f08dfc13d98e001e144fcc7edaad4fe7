package com.files.entites;

import javax.persistence.Entity;

import org.hibernate.annotations.Table;

@Entity
@Table(name="portfolio")
public class recivedPersistenet 
{

	String name;
	String email;
	String message;
	
	public recivedPersistenet() {
		// TODO Auto-generated constructor stub
	}
	
	public recivedPersistenet(String name, String email, String message) {
		super();
		this.name = name;
		this.email = email;
		this.message = message;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public String getMessage() {
		return message;
	}

	public void setMessage(String message) {
		this.message = message;
	}

	@Override
	public String toString() {
		return "recivedPersistenet [name=" + name + ", email=" + email + ", message=" + message + "]";
	}
	
}
